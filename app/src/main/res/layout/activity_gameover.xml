<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
	android:background="@drawable/ic_background_gradient"
	tools:context=".Gameover">
	
	<TextView
			android:id="@+id/game_mode_value2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:fontFamily="@font/big_john"
			android:text="@string/Scores"
			android:textColor="@color/wizardWhite"
			android:textSize="20sp"
			app:layout_constraintEnd_toEndOf="parent"
		android:layout_marginTop="32dp"
		app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent"/>
	<Button
		android:id="@+id/menu"
			android:layout_width="186dp"
			android:layout_height="0dp"
			android:layout_marginStart="16dp"
			android:layout_marginBottom="8dp"
			android:fontFamily="@font/big_john"
			android:text="@string/back_to_menu"
			android:textColor="@color/ButtonTextColor"
			android:textSize="18sp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="@+id/guideline18"
		android:layout_marginTop="8dp"
		android:layout_marginEnd="16dp"
		android:backgroundTint="@color/ButtonColor"
		android:stateListAnimator="@null"
		app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintVertical_bias="1.0"/>

	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/guideline18"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		app:layout_constraintGuide_percent="0.8508892" />
	<androidx.recyclerview.widget.RecyclerView
		android:id="@+id/score_recycleViewer"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:layout_marginStart="24dp"
			android:layout_marginEnd="24dp"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginBottom="8dp"
			android:layout_marginTop="8dp"
		app:layout_constraintBottom_toTopOf="@+id/guideline18"
		app:layout_constraintTop_toTopOf="@+id/guideline19" />
	<ImageView
			android:layout_width="wrap_content"
			android:layout_height="0dp" app:srcCompat="@drawable/ic_profile"
		android:id="@+id/second"
		android:layout_marginStart="8dp"
		app:layout_constraintEnd_toStartOf="@+id/guideline22"
			app:layout_constraintBottom_toTopOf="@+id/guideline19"
		app:layout_constraintHorizontal_bias="0.7"
		app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="@+id/guideline23"/>
	<ImageView
			android:layout_width="wrap_content"
		android:id="@+id/third"
		android:layout_height="0dp"
		android:layout_marginEnd="8dp"
			app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintBottom_toTopOf="@+id/guideline19"
		app:layout_constraintHorizontal_bias="0.3"
		app:layout_constraintStart_toStartOf="@+id/guideline22"
		app:srcCompat="@drawable/ic_profile"
			app:layout_constraintTop_toTopOf="@+id/guideline23"/>

	<ImageView
		android:id="@+id/first"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:layout_marginStart="8dp"
		android:layout_marginTop="8dp"
		android:layout_marginEnd="8dp"
		android:layout_marginBottom="8dp"
		app:layout_constraintBottom_toTopOf="@+id/guideline23"
		app:layout_constraintEnd_toStartOf="@+id/guideline21"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="@+id/guideline20"
		app:layout_constraintTop_toBottomOf="@+id/game_mode_value2"
		app:layout_constraintVertical_bias="0.0"
		app:srcCompat="@drawable/ic_profile" />

	<androidx.constraintlayout.widget.Guideline
			android:orientation="horizontal"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
		android:id="@+id/guideline19"
		app:layout_constraintGuide_percent="0.43" />
	<androidx.constraintlayout.widget.Guideline
			android:orientation="vertical"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
		android:id="@+id/guideline20"
		app:layout_constraintGuide_percent="0.25" />
	<androidx.constraintlayout.widget.Guideline
			android:orientation="vertical"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
		android:id="@+id/guideline21"
		app:layout_constraintGuide_percent="0.75" />
	<androidx.constraintlayout.widget.Guideline
			android:orientation="vertical"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
		android:id="@+id/guideline22"
		app:layout_constraintGuide_percent="0.50" />
	<androidx.constraintlayout.widget.Guideline
			android:orientation="horizontal"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
		android:id="@+id/guideline23"
		app:layout_constraintGuide_percent="0.28" />
	<TextView
			android:text="@string/results_mvp"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView9"
			android:layout_marginBottom="8dp"
		android:fontFamily="@font/big_john"
		android:textColor="@color/wizardTransWhite"
		android:textSize="18sp"
		app:layout_constraintBottom_toTopOf="@+id/guideline23"
		app:layout_constraintEnd_toEndOf="@+id/first"
		app:layout_constraintStart_toStartOf="@+id/first" />
	<TextView
			android:text="@string/results_second"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
		android:id="@+id/textView10"
		android:layout_marginBottom="8dp"
		android:fontFamily="@font/big_john"
		android:textColor="@color/wizardTransWhite"
		android:textSize="14sp"
		app:layout_constraintBottom_toTopOf="@+id/guideline19"
		app:layout_constraintEnd_toEndOf="@+id/second"
		app:layout_constraintStart_toStartOf="@+id/second" />
	<TextView
			android:text="@string/results_third"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
		android:id="@+id/textView11"
		android:layout_marginBottom="8dp"
		android:fontFamily="@font/big_john"
		android:textColor="@color/wizardTransWhite"
		android:textSize="14sp"
		app:layout_constraintBottom_toTopOf="@+id/guideline19"
		app:layout_constraintEnd_toEndOf="@+id/third"
		app:layout_constraintStart_toStartOf="@+id/third" />
	<TextView
		android:id="@+id/mvp_name"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
		android:layout_marginTop="8dp"
		android:fontFamily="@font/big_john"
		android:text="@string/textview"
		android:textAlignment="center"
		android:textColor="@color/wizardWhite"
		app:layout_constraintEnd_toEndOf="@+id/first"
		app:layout_constraintStart_toStartOf="@+id/first"
		app:layout_constraintTop_toBottomOf="@+id/game_mode_value2" />
	<TextView
		android:id="@+id/second_name"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
		android:layout_marginTop="8dp"
		android:fontFamily="@font/big_john"
		android:text="@string/textview"
		android:textAlignment="center"
		android:textColor="@color/wizardWhite"
		app:layout_constraintEnd_toEndOf="@+id/second"
		app:layout_constraintStart_toStartOf="@+id/second"
		app:layout_constraintTop_toTopOf="@+id/guideline23" />
	<TextView
		android:id="@+id/third_name"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
		android:layout_marginTop="8dp"
		android:fontFamily="@font/big_john"
		android:text="@string/textview"
		android:textAlignment="center"
		android:textColor="@color/wizardWhite"
		app:layout_constraintEnd_toEndOf="@+id/third"
		app:layout_constraintStart_toStartOf="@+id/third"
		app:layout_constraintTop_toTopOf="@+id/guideline23" />
</androidx.constraintlayout.widget.ConstraintLayout>